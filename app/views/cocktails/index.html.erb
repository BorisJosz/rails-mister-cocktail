<h1>Cocktails</h1>
<% @cocktails.each do |cocktail| %>
<ul>
  <li>
    <%= link_to cocktail_path(cocktail) do %>
    <%= cocktail.name %>
    <% end %>
    <%= cl_image_tag cocktail.photo, height: 50, width: 50, crop: :fill %>
  </li>
</ul>
<% end %>

<%= link_to 'Add new cocktail', new_cocktail_path %>
